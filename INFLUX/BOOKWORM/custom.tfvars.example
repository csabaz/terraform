pm_api_url = "https://xxx.xxx.xxx.xxx:8006/api2/json"
pm_api_token_id = "user@pve!token"
pm_api_token_secret = "xxxxxxxxxx-xxxxxx-xxxxxxx-xxxxx"
pm_tls_insecure = "true"

proxmox_ip = "xxx.xxx.xxx.xxx"
ssh_user = ""
ssh_key = "ssh-rsa xxx"


#count = "1"
name = "influx"
vm_id_prefix = "20"
target_node = "pve"

# Clone from xxx-ci-x64-influx template
clone = "bookworm-ci-x64-influx"
os_type = "cloud-init"
cicustom = "user=btrfs:snippets/cloud_init_influx.yml"
##ipconfig0 = "ip=${var.ipre}${count.index + <NUBER es: 1>}/24,gw=${var.gw}"
ipre = "xxx.xxx.xxx.XX"
mask = "24"
gateway = "xxx.xxx.xxx.xxx"

cpu = "host"
sockets = "1"
cores = "4"
memory = "16384"
agent = "1"

bootdisk = "scsi0"
scsihw = "virtio-scsi-pci"

size = "128G"
type = "scsi"
storage = "local-btrfs"
iothread = "0"

model = "virtio"
bridge = "vmbr0"

dns_home_var = "xxx.xxx.xxx.xxx"
search_domains_home_var = "xxxxxxx.xxx"

host_home_influx="xxx.xxx.xxx.xxx influx.xxxx.net influx"

influx_org    = "xxx-db"
influx_bucket = "xxx-bucket"
influx_user   = "xxxxxx"
influx_pass   = "xxxxxxxxxxxxxx"
